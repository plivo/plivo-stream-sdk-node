{
  "name": "plivo-stream-sdk-node",
  "version": "1.0.0",
  "description": "A Node.js SDK for handling Plivo real-time media streaming over WebSocket",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "plivo",
    "streaming",
    "websocket",
    "real-time",
    "media",
    "audio",
    "voice"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.25",
    "@types/ws": "^8.5.8",
    "nodemon": "^3.1.11",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "plivo": "^4.75.3",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  }
}
